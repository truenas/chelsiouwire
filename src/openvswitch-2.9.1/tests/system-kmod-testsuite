$at_failed && at_fn_log_failure  \
"ovsdb-server.log" \
"ovs-vswitchd.log"
$at_traceon; }


      fi
      if test -n "\
         "192.168.1.1""; then
         { set +x
$as_echo "$at_srcdir/system-ovn.at:1008: ip netns exec foo1 sh << NS_EXEC_HEREDOC
ip route add default via \\
         \"192.168.1.1\"
NS_EXEC_HEREDOC"
at_fn_check_prepare_notrace 'an embedded newline' "system-ovn.at:1008"
