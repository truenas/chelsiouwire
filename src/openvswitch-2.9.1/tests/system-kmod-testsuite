        PROTO=$(echo ftp | sed -e 's/\([a-zA-Z]*\).*/\1/')
    $as_echo "system-traffic.at:3423: waiting until ip netns exec at_ns1 sh << NS_EXEC_HEREDOC
netstat -l | grep \$PROTO
NS_EXEC_HEREDOC..." >&5
ovs_wait_cond () {
    ip netns exec at_ns1 sh << NS_EXEC_HEREDOC
netstat -l | grep $PROTO
NS_EXEC_HEREDOC
}
if ovs_wait; then :
else
    $as_echo "system-traffic.at:3423: wait failed" >&5

    $as_echo "system-traffic.at:3423" >"$at_check_line_file"
at_fn_check_skip 99 "$at_srcdir/system-traffic.at:3423"
fi




         { set +x
$as_echo "$at_srcdir/system-traffic.at:3423: ip netns exec at_ns0 sh << NS_EXEC_HEREDOC
wget ftp://10.1.1.2 -4 --no-passive-ftp -t 3 -T 1 --retry-connrefused -v --server-response --no-remove-listing -o wget0.log -d
NS_EXEC_HEREDOC"
