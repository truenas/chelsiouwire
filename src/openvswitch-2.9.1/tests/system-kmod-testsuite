ovn-nbctl --wait=hv sync
$as_echo "system-ovn.at:1462: waiting until ovs-ofctl dump-flows br-int | grep 'nat(src=172.16.1.1)'..." >&5
ovs_wait_cond () {
    ovs-ofctl dump-flows br-int | grep 'nat(src=172.16.1.1)'
}
if ovs_wait; then :
else
    $as_echo "system-ovn.at:1462: wait failed" >&5

    $as_echo "system-ovn.at:1462" >"$at_check_line_file"
at_fn_check_skip 99 "$at_srcdir/system-ovn.at:1462"
fi


echo "------ hv dump ------"
ovs-ofctl show br-int
ovs-ofctl dump-flows br-int
echo "---------------------"

# East-West No NAT: 'foo1' pings 'bar1' using 192.168.2.2.
 { set +x
$as_echo "$at_srcdir/system-ovn.at:1470: ip netns exec foo1 sh << NS_EXEC_HEREDOC
ping -q -c 3 -i 0.3 -w 2 192.168.2.2 | grep \"transmitted\" | sed 's/time.*ms\$/time 0ms/'
NS_EXEC_HEREDOC"
at_fn_check_prepare_notrace 'an embedded newline' "system-ovn.at:1470"
( $at_check_trace; ip netns exec foo1 sh << NS_EXEC_HEREDOC
ping -q -c 3 -i 0.3 -w 2 192.168.2.2 | grep "transmitted" | sed 's/time.*ms$/time 0ms/'
NS_EXEC_HEREDOC
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo >>"$at_stdout"; $as_echo "3 packets transmitted, 3 received, 0% packet loss, time 0ms
" | \
  $at_diff - "$at_stdout" || at_failed=:
at_fn_check_status \
0 $at_status "$at_srcdir/system-ovn.at:1470"
$at_failed && at_fn_log_failure  \
"ovsdb-server.log" \
"ovs-vswitchd.log"
$at_traceon; }



# We verify that no NAT happened via 'dump-conntrack' command.
{ set +x
$as_echo "$at_srcdir/system-ovn.at:1476: ovs-appctl dpctl/dump-conntrack | grep \"dst=192.168.2.2\" | sed -e 's/port=[0-9]*/port=<cleared>/g' -e 's/id=[0-9]*/id=<cleared>/g' -e 's/state=[0-9_A-Z]*/state=<cleared>/g' | sort | uniq | \\
sed -e 's/zone=[0-9]*/zone=<cleared>/' | wc -l"
at_fn_check_prepare_notrace 'an embedded newline' "system-ovn.at:1476"
( $at_check_trace; ovs-appctl dpctl/dump-conntrack | grep "dst=192.168.2.2" | sed -e 's/port=[0-9]*/port=<cleared>/g' -e 's/id=[0-9]*/id=<cleared>/g' -e 's/state=[0-9_A-Z]*/state=<cleared>/g' | sort | uniq | \
sed -e 's/zone=[0-9]*/zone=<cleared>/' | wc -l
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo >>"$at_stdout"; $as_echo "0
" | \
  $at_diff - "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/system-ovn.at:1476"
$at_failed && at_fn_log_failure  \
"ovsdb-server.log" \
"ovs-vswitchd.log"
$at_traceon; }


