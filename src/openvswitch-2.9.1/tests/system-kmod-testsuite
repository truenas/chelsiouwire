#AT_START_92
at_fn_group_banner 92 'system-layer3-tunnels.at:52' \
  "layer3 - ping over GRE" "                         " 6
at_xfail=no
(
  $as_echo "92. $at_setup_line: testing $at_desc ..."
  $at_traceon

ovs_init

{ set +x
$as_echo "$at_srcdir/system-layer3-tunnels.at:53: modprobe openvswitch"
