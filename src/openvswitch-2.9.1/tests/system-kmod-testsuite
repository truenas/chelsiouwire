$as_echo "system-traffic.at:2067: waiting until ip netns exec at_ns0 ping6 -c 1 fc00:1::4..." >&5
ovs_wait_cond () {
    ip netns exec at_ns0 ping6 -c 1 fc00:1::4
}
if ovs_wait; then :
else
