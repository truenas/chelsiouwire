$as_echo "system-traffic.at:4316: waiting until ip netns exec at_ns0 ping -c 1 10.2.2.2..." >&5
ovs_wait_cond () {
    ip netns exec at_ns0 ping -c 1 10.2.2.2
}
if ovs_wait; then :
else
    $as_echo "system-traffic.at:4316: wait failed" >&5

    $as_echo "system-traffic.at:4316" >"$at_check_line_file"
at_fn_check_skip 99 "$at_srcdir/system-traffic.at:4316"
fi


 { set +x
$as_echo "$at_srcdir/system-traffic.at:4318: ip netns exec at_ns0 sh << NS_EXEC_HEREDOC
ping -q -c 3 -i 0.3 -w 2 10.2.2.2 | grep \"transmitted\" | sed 's/time.*ms\$/time 0ms/'
NS_EXEC_HEREDOC"
at_fn_check_prepare_notrace 'an embedded newline' "system-traffic.at:4318"
( $at_check_trace; ip netns exec at_ns0 sh << NS_EXEC_HEREDOC
ping -q -c 3 -i 0.3 -w 2 10.2.2.2 | grep "transmitted" | sed 's/time.*ms$/time 0ms/'
NS_EXEC_HEREDOC
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
