$at_failed && at_fn_log_failure  \
"ovsdb-server.log" \
"ovs-vswitchd.log"
$at_traceon; }

   TMPPID=$(cat $OVS_RUNDIR/ovn-controller.pid)
   { set +x
$as_echo "$at_srcdir/system-ovn.at:1530: ovs-appctl --timeout=10 -t ovn-controller exit"
at_fn_check_prepare_trace "system-ovn.at:1530"
( $at_check_trace; ovs-appctl --timeout=10 -t ovn-controller exit
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/system-ovn.at:1530"
$at_failed && at_fn_log_failure  \
"ovsdb-server.log" \
"ovs-vswitchd.log"
$at_traceon; }

   $as_echo "system-ovn.at:1530: waiting while kill -0 \$TMPPID 2>/dev/null..." >&5
ovs_wait_cond () {
    if kill -0 $TMPPID 2>/dev/null; then return 1; else return 0; fi
}
if ovs_wait; then :
else
    $as_echo "system-ovn.at:1530: wait failed" >&5

    $as_echo "system-ovn.at:1530" >"$at_check_line_file"
at_fn_check_skip 99 "$at_srcdir/system-ovn.at:1530"
fi


