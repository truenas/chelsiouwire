NS_EXEC_HEREDOC..." >&5
ovs_wait_cond () {
    ip netns exec at_ns1 sh << NS_EXEC_HEREDOC
netstat -l | grep $PROTO
NS_EXEC_HEREDOC
}
if ovs_wait; then :
else
    $as_echo "system-traffic.at:3357: wait failed" >&5

    $as_echo "system-traffic.at:3357" >"$at_check_line_file"
at_fn_check_skip 99 "$at_srcdir/system-traffic.at:3357"
fi




         { set +x
$as_echo "$at_srcdir/system-traffic.at:3357: ip netns exec at_ns0 sh << NS_EXEC_HEREDOC
