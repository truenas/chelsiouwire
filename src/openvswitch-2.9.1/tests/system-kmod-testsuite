$at_traceon; }

   $as_echo "system-traffic.at:3177: waiting while kill -0 \$TMPPID 2>/dev/null..." >&5
ovs_wait_cond () {
    if kill -0 $TMPPID 2>/dev/null; then return 1; else return 0; fi
}
if ovs_wait; then :
else
    $as_echo "system-traffic.at:3177: wait failed" >&5

    $as_echo "system-traffic.at:3177" >"$at_check_line_file"
