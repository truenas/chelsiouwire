$as_echo "system-traffic.at:1983: waiting until ip netns exec at_ns0 ping6 -c 1 fc00::2..." >&5
ovs_wait_cond () {
    ip netns exec at_ns0 ping6 -c 1 fc00::2
}
if ovs_wait; then :
else
    $as_echo "system-traffic.at:1983: wait failed" >&5

    $as_echo "system-traffic.at:1983" >"$at_check_line_file"
at_fn_check_skip 99 "$at_srcdir/system-traffic.at:1983"
fi


 { set +x
$as_echo "$at_srcdir/system-traffic.at:1986: ip netns exec at_ns0 sh << NS_EXEC_HEREDOC
ping6 -s 1600 -q -c 1 -i 0.3 -w 2 fc00::2 | grep \"transmitted\" | sed 's/time.*ms\$/time 0ms/'
NS_EXEC_HEREDOC"
at_fn_check_prepare_notrace 'an embedded newline' "system-traffic.at:1986"
( $at_check_trace; ip netns exec at_ns0 sh << NS_EXEC_HEREDOC
ping6 -s 1600 -q -c 1 -i 0.3 -w 2 fc00::2 | grep "transmitted" | sed 's/time.*ms$/time 0ms/'
NS_EXEC_HEREDOC
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo >>"$at_stdout"; $as_echo "7 packets transmitted, 0 received, 100% packet loss, time 0ms
" | \
  $at_diff - "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/system-traffic.at:1986"
$at_failed && at_fn_log_failure  \
"ovsdb-server.log" \
"ovs-vswitchd.log"
$at_traceon; }




