at_fn_check_status 0 $at_status "$at_srcdir/system-ovn.at:748"
$at_failed && at_fn_log_failure  \
"ovsdb-server.log" \
"ovs-vswitchd.log"
$at_traceon; }

   TMPPID=$(cat $OVS_RUNDIR/ovn-controller.pid)
   { set +x
$as_echo "$at_srcdir/system-ovn.at:748: ovs-appctl --timeout=10 -t ovn-controller exit"
at_fn_check_prepare_trace "system-ovn.at:748"
( $at_check_trace; ovs-appctl --timeout=10 -t ovn-controller exit
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
