at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/system-traffic.at:3357"
$at_failed && at_fn_log_failure  \
"ovsdb-server.log" \
"ovs-vswitchd.log"
$at_traceon; }

   $as_echo "system-traffic.at:3357: waiting while kill -0 \$TMPPID 2>/dev/null..." >&5
ovs_wait_cond () {
    if kill -0 $TMPPID 2>/dev/null; then return 1; else return 0; fi
}
if ovs_wait; then :
else
    $as_echo "system-traffic.at:3357: wait failed" >&5

    $as_echo "system-traffic.at:3357" >"$at_check_line_file"
at_fn_check_skip 99 "$at_srcdir/system-traffic.at:3357"
fi

   { set +x
$as_echo "$at_srcdir/system-traffic.at:3357: test -e \$OVS_RUNDIR/ovsdb-server.pid"
at_fn_check_prepare_dynamic "test -e $OVS_RUNDIR/ovsdb-server.pid" "system-traffic.at:3357"
( $at_check_trace; test -e $OVS_RUNDIR/ovsdb-server.pid
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
