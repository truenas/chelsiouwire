$at_traceon; }


      { set +x
$as_echo "$at_srcdir/system-ovn.at:1089: ovs-vsctl -- add-br br0 -- set Bridge br0 protocols=OpenFlow10,OpenFlow11,OpenFlow12,OpenFlow13,OpenFlow14,OpenFlow15 fail-mode=secure  --  "
at_fn_check_prepare_trace "system-ovn.at:1089"
( $at_check_trace; ovs-vsctl -- add-br br0 -- set Bridge br0 protocols=OpenFlow10,OpenFlow11,OpenFlow12,OpenFlow13,OpenFlow14,OpenFlow15 fail-mode=secure  --
