#AT_START_38
at_fn_group_banner 38 'system-traffic.at:1653' \
  "conntrack - ICMP related to original direction" " " 2
at_xfail=no
(
  $as_echo "38. $at_setup_line: testing $at_desc ..."
  $at_traceon

ovs_init

$as_echo "system-traffic.at:1654" >"$at_check_line_file"
(test $HAVE_NC = no) \
  && at_fn_check_skip 77 "$at_srcdir/system-traffic.at:1654"
