"ovs-vswitchd.log"
$at_traceon; }


       { set +x
$as_echo "$at_srcdir/system-ovn.at:832: ip netns exec foo1 sh << NS_EXEC_HEREDOC
ip link set dev foo1 up
NS_EXEC_HEREDOC"
