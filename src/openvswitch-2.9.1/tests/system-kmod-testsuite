PIDFILE=$(mktemp httpXXX.pid)
    ip netns exec at_ns1 $PYTHON $srcdir/test-l7.py http & echo $! > $PIDFILE
     echo "kill \`cat $PIDFILE\`" >> cleanup



        PROTO=$(echo http | sed -e 's/\([a-zA-Z]*\).*/\1/')
    $as_echo "system-traffic.at:3001: waiting until ip netns exec at_ns1 sh << NS_EXEC_HEREDOC
netstat -l | grep \$PROTO
NS_EXEC_HEREDOC..." >&5
ovs_wait_cond () {
    ip netns exec at_ns1 sh << NS_EXEC_HEREDOC
netstat -l | grep $PROTO
NS_EXEC_HEREDOC
}
if ovs_wait; then :
else
    $as_echo "system-traffic.at:3001: wait failed" >&5

    $as_echo "system-traffic.at:3001" >"$at_check_line_file"
at_fn_check_skip 99 "$at_srcdir/system-traffic.at:3001"
fi



 { set +x
$as_echo "$at_srcdir/system-traffic.at:3002: ip netns exec at_ns0 sh << NS_EXEC_HEREDOC
