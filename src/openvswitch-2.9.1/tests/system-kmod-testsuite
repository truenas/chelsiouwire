PIDFILE=$(mktemp tftpXXX.pid)
    ip netns exec at_ns0 $PYTHON $srcdir/test-l7.py tftp & echo $! > $PIDFILE
     echo "kill \`cat $PIDFILE\`" >> cleanup



        PROTO=$(echo tftp | sed -e 's/\([a-zA-Z]*\).*/\1/')
    $as_echo "system-traffic.at:2841: waiting until ip netns exec at_ns0 sh << NS_EXEC_HEREDOC
netstat -l | grep \$PROTO
NS_EXEC_HEREDOC..." >&5
ovs_wait_cond () {
    ip netns exec at_ns0 sh << NS_EXEC_HEREDOC
netstat -l | grep $PROTO
NS_EXEC_HEREDOC
}
if ovs_wait; then :
else
    $as_echo "system-traffic.at:2841: wait failed" >&5

    $as_echo "system-traffic.at:2841" >"$at_check_line_file"
at_fn_check_skip 99 "$at_srcdir/system-traffic.at:2841"
fi



PIDFILE=$(mktemp tftpXXX.pid)
    ip netns exec at_ns1 $PYTHON $srcdir/test-l7.py tftp & echo $! > $PIDFILE
     echo "kill \`cat $PIDFILE\`" >> cleanup



        PROTO=$(echo tftp | sed -e 's/\([a-zA-Z]*\).*/\1/')
    $as_echo "system-traffic.at:2842: waiting until ip netns exec at_ns1 sh << NS_EXEC_HEREDOC
netstat -l | grep \$PROTO
NS_EXEC_HEREDOC..." >&5
ovs_wait_cond () {
    ip netns exec at_ns1 sh << NS_EXEC_HEREDOC
netstat -l | grep $PROTO
NS_EXEC_HEREDOC
}
if ovs_wait; then :
else
    $as_echo "system-traffic.at:2842: wait failed" >&5

    $as_echo "system-traffic.at:2842" >"$at_check_line_file"
at_fn_check_skip 99 "$at_srcdir/system-traffic.at:2842"
fi




