   TMPPID=$(cat $OVS_RUNDIR/ovs-vswitchd.pid)
   { set +x
$as_echo "$at_srcdir/system-traffic.at:3357: ovs-appctl --timeout=10 -t ovs-vswitchd exit --cleanup"
