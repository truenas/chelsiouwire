{ set +x
$as_echo "$at_srcdir/system-traffic.at:3105: ovs-appctl dpctl/dump-conntrack | grep \"dst=10.1.1.64\" | sed -e 's/port=[0-9]*/port=<cleared>/g' -e 's/id=[0-9]*/id=<cleared>/g' -e 's/state=[0-9_A-Z]*/state=<cleared>/g' | sort | uniq"
