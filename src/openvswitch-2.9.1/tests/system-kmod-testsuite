     echo "kill \`cat ftp0.pid\`" >> cleanup


$as_echo "system-traffic.at:2501: waiting until ip netns exec at_ns1 netstat -l | grep ftp..." >&5
ovs_wait_cond () {
    ip netns exec at_ns1 netstat -l | grep ftp
}
if ovs_wait; then :
else
    $as_echo "system-traffic.at:2501: wait failed" >&5

    $as_echo "system-traffic.at:2501" >"$at_check_line_file"
at_fn_check_skip 99 "$at_srcdir/system-traffic.at:2501"
