   TMPPID=$(cat $OVS_RUNDIR/ovs-vswitchd.pid)
   { set +x
$as_echo "$at_srcdir/system-traffic.at:4445: ovs-appctl --timeout=10 -t ovs-vswitchd exit --cleanup"
at_fn_check_prepare_trace "system-traffic.at:4445"
( $at_check_trace; ovs-appctl --timeout=10 -t ovs-vswitchd exit --cleanup
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
