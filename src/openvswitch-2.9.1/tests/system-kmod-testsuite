    ip netns exec at_ns1 sh << NS_EXEC_HEREDOC
netstat -l | grep $PROTO
NS_EXEC_HEREDOC
}
if ovs_wait; then :
else
    $as_echo "system-traffic.at:2567: wait failed" >&5

    $as_echo "system-traffic.at:2567" >"$at_check_line_file"
