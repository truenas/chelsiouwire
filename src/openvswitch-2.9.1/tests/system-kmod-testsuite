$as_echo "system-traffic.at:4423: waiting until ip netns exec at_ns0 ping -c 1 10.2.2.2..." >&5
ovs_wait_cond () {
    ip netns exec at_ns0 ping -c 1 10.2.2.2
}
if ovs_wait; then :
else
    $as_echo "system-traffic.at:4423: wait failed" >&5

    $as_echo "system-traffic.at:4423" >"$at_check_line_file"
at_fn_check_skip 99 "$at_srcdir/system-traffic.at:4423"
fi

