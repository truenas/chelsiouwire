$as_echo "system-traffic.at:2183: waiting until ip netns exec at_ns0 ping6 -c 1 fc00::2..." >&5
ovs_wait_cond () {
    ip netns exec at_ns0 ping6 -c 1 fc00::2
}
if ovs_wait; then :
else
    $as_echo "system-traffic.at:2183: wait failed" >&5

    $as_echo "system-traffic.at:2183" >"$at_check_line_file"
at_fn_check_skip 99 "$at_srcdir/system-traffic.at:2183"
fi


 { set +x
