$as_echo "system-traffic.at:2042" >"$at_check_line_file"
(! grep -q "VLAN header stack length probed as" ovs-vswitchd.log) \
  && at_fn_check_skip 77 "$at_srcdir/system-traffic.at:2042"
     $as_echo "system-traffic.at:2042" >"$at_check_line_file"
