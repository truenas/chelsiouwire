ovn-nbctl lsp-add alice alice1 \
-- lsp-set-addresses alice1 "f0:00:00:01:02:05 172.16.1.2"

# Config OVN load-balancer with a VIP.
uuid=`ovn-nbctl  create load_balancer vips:172.16.1.10="192.168.1.2,192.168.2.2"`
ovn-nbctl set logical_router R1 load_balancer=$uuid

# Config OVN load-balancer with another VIP (this time with ports).
ovn-nbctl set load_balancer $uuid vips:'"172.16.1.11:8000"'='"192.168.1.2:80,192.168.2.2:80"'

# Wait for ovn-controller to catch up.
ovn-nbctl --wait=hv sync
$as_echo "system-ovn.at:1175: waiting until ovs-ofctl -O OpenFlow13 dump-groups br-int | \\
grep 'nat(dst=192.168.2.2:80)'..." >&5
ovs_wait_cond () {
    ovs-ofctl -O OpenFlow13 dump-groups br-int | \
grep 'nat(dst=192.168.2.2:80)'
}
if ovs_wait; then :
else
    $as_echo "system-ovn.at:1175: wait failed" >&5

    $as_echo "system-ovn.at:1175" >"$at_check_line_file"
at_fn_check_skip 99 "$at_srcdir/system-ovn.at:1175"
fi


# Start webservers in 'foo1', 'bar1'.
PIDFILE=$(mktemp httpXXX.pid)
    ip netns exec foo1 $PYTHON $srcdir/test-l7.py http & echo $! > $PIDFILE
     echo "kill \`cat $PIDFILE\`" >> cleanup



        PROTO=$(echo http | sed -e 's/\([a-zA-Z]*\).*/\1/')
    $as_echo "system-ovn.at:1179: waiting until ip netns exec foo1 sh << NS_EXEC_HEREDOC
netstat -l | grep \$PROTO
NS_EXEC_HEREDOC..." >&5
ovs_wait_cond () {
    ip netns exec foo1 sh << NS_EXEC_HEREDOC
netstat -l | grep $PROTO
NS_EXEC_HEREDOC
}
if ovs_wait; then :
else
    $as_echo "system-ovn.at:1179: wait failed" >&5

    $as_echo "system-ovn.at:1179" >"$at_check_line_file"
at_fn_check_skip 99 "$at_srcdir/system-ovn.at:1179"
fi



PIDFILE=$(mktemp httpXXX.pid)
    ip netns exec bar1 $PYTHON $srcdir/test-l7.py http & echo $! > $PIDFILE
     echo "kill \`cat $PIDFILE\`" >> cleanup



        PROTO=$(echo http | sed -e 's/\([a-zA-Z]*\).*/\1/')
    $as_echo "system-ovn.at:1180: waiting until ip netns exec bar1 sh << NS_EXEC_HEREDOC
netstat -l | grep \$PROTO
NS_EXEC_HEREDOC..." >&5
ovs_wait_cond () {
    ip netns exec bar1 sh << NS_EXEC_HEREDOC
netstat -l | grep $PROTO
NS_EXEC_HEREDOC
}
if ovs_wait; then :
else
    $as_echo "system-ovn.at:1180: wait failed" >&5

    $as_echo "system-ovn.at:1180" >"$at_check_line_file"
at_fn_check_skip 99 "$at_srcdir/system-ovn.at:1180"
fi




for i in `seq 1 20`; do
    echo Request $i
     { set +x
$as_echo "$at_srcdir/system-ovn.at:1185: ip netns exec alice1 sh << NS_EXEC_HEREDOC
wget 172.16.1.10 -t 5 -T 1 --retry-connrefused -v -o wget\$i.log
NS_EXEC_HEREDOC"
at_fn_check_prepare_notrace 'an embedded newline' "system-ovn.at:1185"
( $at_check_trace; ip netns exec alice1 sh << NS_EXEC_HEREDOC
wget 172.16.1.10 -t 5 -T 1 --retry-connrefused -v -o wget$i.log
NS_EXEC_HEREDOC
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/system-ovn.at:1185"
$at_failed && at_fn_log_failure  \
"ovsdb-server.log" \
"ovs-vswitchd.log"
$at_traceon; }


done

{ set +x
$as_echo "$at_srcdir/system-ovn.at:1189: ovs-appctl dpctl/dump-conntrack | grep \"dst=172.16.1.10\" | sed -e 's/port=[0-9]*/port=<cleared>/g' -e 's/id=[0-9]*/id=<cleared>/g' -e 's/state=[0-9_A-Z]*/state=<cleared>/g' | sort | uniq |
sed -e 's/zone=[0-9]*/zone=<cleared>/'"
at_fn_check_prepare_notrace 'an embedded newline' "system-ovn.at:1189"
( $at_check_trace; ovs-appctl dpctl/dump-conntrack | grep "dst=172.16.1.10" | sed -e 's/port=[0-9]*/port=<cleared>/g' -e 's/id=[0-9]*/id=<cleared>/g' -e 's/state=[0-9_A-Z]*/state=<cleared>/g' | sort | uniq |
sed -e 's/zone=[0-9]*/zone=<cleared>/'
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
echo >>"$at_stdout"; $as_echo "tcp,orig=(src=172.16.1.2,dst=172.16.1.10,sport=<cleared>,dport=<cleared>),reply=(src=192.168.1.2,dst=172.16.1.2,sport=<cleared>,dport=<cleared>),zone=<cleared>,protoinfo=(state=<cleared>)
tcp,orig=(src=172.16.1.2,dst=172.16.1.10,sport=<cleared>,dport=<cleared>),reply=(src=192.168.2.2,dst=172.16.1.2,sport=<cleared>,dport=<cleared>),zone=<cleared>,protoinfo=(state=<cleared>)
" | \
  $at_diff - "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/system-ovn.at:1189"
$at_failed && at_fn_log_failure  \
"ovsdb-server.log" \
"ovs-vswitchd.log"
$at_traceon; }


for i in `seq 1 20`; do
    echo Request $i
     { set +x
$as_echo "$at_srcdir/system-ovn.at:1198: ip netns exec alice1 sh << NS_EXEC_HEREDOC
wget 172.16.1.11:8000 -t 5 -T 1 --retry-connrefused -v -o wget\$i.log
NS_EXEC_HEREDOC"
