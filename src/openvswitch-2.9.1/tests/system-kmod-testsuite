               on_exit 'modprobe -q -r vport_stt'
              modprobe -q vport_vxlan || echo "Module vport_vxlan not loaded."
               on_exit 'modprobe -q -r vport_vxlan'

   on_exit 'ovs-dpctl del-dp ovs-system'
   on_exit 'ovs-appctl dpctl/flush-conntrack'
      touch .conf.db.~lock~
   { set +x
