$as_echo "system-traffic.at:3638: waiting until ip netns exec at_ns0 ping6 -c 1 fc00::2..." >&5
ovs_wait_cond () {
    ip netns exec at_ns0 ping6 -c 1 fc00::2
}
if ovs_wait; then :
else
    $as_echo "system-traffic.at:3638: wait failed" >&5

    $as_echo "system-traffic.at:3638" >"$at_check_line_file"
at_fn_check_skip 99 "$at_srcdir/system-traffic.at:3638"
fi


PIDFILE=$(mktemp http6XXX.pid)
    ip netns exec at_ns1 $PYTHON $srcdir/test-l7.py http6 & echo $! > $PIDFILE
     echo "kill \`cat $PIDFILE\`" >> cleanup



        PROTO=$(echo http6 | sed -e 's/\([a-zA-Z]*\).*/\1/')
