$as_echo "system-traffic.at:3794: waiting until ip netns exec at_ns0 ping6 -c 1 fc00::2 >/dev/null..." >&5
ovs_wait_cond () {
    ip netns exec at_ns0 ping6 -c 1 fc00::2 >/dev/null
}
if ovs_wait; then :
else
    $as_echo "system-traffic.at:3794: wait failed" >&5

    $as_echo "system-traffic.at:3794" >"$at_check_line_file"
at_fn_check_skip 99 "$at_srcdir/system-traffic.at:3794"
fi


PIDFILE=$(mktemp ftpXXX.pid)
    ip netns exec at_ns1 $PYTHON $srcdir/test-l7.py ftp & echo $! > $PIDFILE
     echo "kill \`cat $PIDFILE\`" >> cleanup



        PROTO=$(echo ftp | sed -e 's/\([a-zA-Z]*\).*/\1/')
    $as_echo "system-traffic.at:3796: waiting until ip netns exec at_ns1 sh << NS_EXEC_HEREDOC
netstat -l | grep \$PROTO
NS_EXEC_HEREDOC..." >&5
ovs_wait_cond () {
    ip netns exec at_ns1 sh << NS_EXEC_HEREDOC
netstat -l | grep $PROTO
NS_EXEC_HEREDOC
}
if ovs_wait; then :
else
    $as_echo "system-traffic.at:3796: wait failed" >&5

    $as_echo "system-traffic.at:3796" >"$at_check_line_file"
at_fn_check_skip 99 "$at_srcdir/system-traffic.at:3796"
fi




