$at_failed && at_fn_log_failure  \
"ovsdb-server.log" \
"ovs-vswitchd.log"
$at_traceon; }


{ set +x
$as_echo "$at_srcdir/system-ovn.at:1530: test -e \$OVS_RUNDIR/ovn-controller.pid"
at_fn_check_prepare_dynamic "test -e $OVS_RUNDIR/ovn-controller.pid" "system-ovn.at:1530"
( $at_check_trace; test -e $OVS_RUNDIR/ovn-controller.pid
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
