     echo "kill \`cat $PIDFILE\`" >> cleanup



        PROTO=$(echo ftp | sed -e 's/\([a-zA-Z]*\).*/\1/')
    $as_echo "system-traffic.at:3357: waiting until ip netns exec at_ns1 sh << NS_EXEC_HEREDOC
